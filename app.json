{
  "expo": {
    "name": "Сэтгэл зүйн апп",
    "slug": "Psychology",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.Uugana.Psychology"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "expo.Uuganaa.Psychology"
    }
  }
}
